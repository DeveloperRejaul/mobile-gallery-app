{
  "expo": {
    "name": "mobile-gallery-app",
    "slug": "mobile-gallery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/core/assets/images/icon.png",
    "scheme": "galleryapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/core/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/core/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rejaul.mobilegalleryapp"
    },
    "web": {
      "favicon": "./src/core/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "extra": {
      "eas": { }
    }
  }
}
